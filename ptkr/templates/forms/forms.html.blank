{% extends "base.html" %}

{% load static %}

{% block content %}

<div class="content">
    <!-- partial:index.partial.html -->
    <div class="container-fluid">
        <div class="justify-content-center">
            <div class="text-center p-0 mt-3 mb-2">
                <div class="card card-outline card-info px-4 pt-4 pb-0 mb-3">
                    <div>
                        <div class="card-tools float-right">
                            <button type="button" class="btn btn-tool" data-toggle="tooltip" data-placement="bottom" title="Bantuan">
                                <i class="fas fa-question-circle fa-lg"></i>
                            </button>
                        </div>
                    </div>
                    <h2 id="heading">FORMULIR INPUT DATA BANGUNAN SATU LANTAI</h2>
                    <p>Masukan data sesuai hasil pengamatan</p>
                    <form id="msform" method="POST" action="" enctype="multipart/form-data">
                        <!-- progressbar -->
                        <ul id="progressbar">
                            <li class="active" id="personal"><strong>Personal</strong></li>
                            <li id="struktur"><strong>Struktur</strong></li>
                            <li id="arsitektur"><strong>Arsitektur</strong></li>
                            <li id="utilitas"><strong>Utilitas</strong></li>
                            <li id="lokasi"><strong>Lokasi</strong></li>
                        </ul>
                        <div class="progress">
                            <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuemin="0" aria-valuemax="100"></div>
                        </div> <br> 
                        <!-- fieldsets -->
                        <fieldset>
                            <div class="form-card">
                                <div class="row">
                                    <div class="col-7">
                                        <h2 class="fs-title">Informasi Dasar:</h2>
                                    </div>
                                    <div class="col-5">
                                        <h2 class="steps">Step 1 - 5</h2>
                                    </div>
                                    <div class="col-md-6">
                                        <label class="fieldlabels" for="namapemilik">Nama Pemilik: *</label> 
                                        <input type="text" class="form-control-sm" id="namapemilik" name="nama_pemilik" placeholder="Nama Pemilik" /> 
                                        <label class="fieldlabels" for="tiperumah">Tipe Rumah: *</label> 
                                        <input type="text" class="form-control-sm" id="tiperumah" name="tipe_rumah" placeholder="Tipe Rumah" /> 
                                        <label class="fieldlabels" for="provinsi">Provinsi: *</label> 
                                        <select name="provinsi" id="provinsi" class="form-control form-control-sm">
                                            <option>Pilih</option>
                                        </select>
                                        <label class="fieldlabels" for="kota">Kota: *</label> 
                                        <select name="kota" id="kota" class="form-control form-control-sm">
                                            <option>Pilih</option>
                                        </select>
                                        <label class="fieldlabels" for="kecamatan">Kecamatan: *</label> 
                                        <select name="kecamatan" id="kecamatan" class="form-control form-control-sm">
                                            <option>Pilih</option>
                                        </select>
                                        <label class="fieldlabels" for="kelurahan">Desa: *</label> 
                                        <select name="kelurahan" id="kelurahan" class="form-control form-control-sm">
                                            <option>Pilih</option>
                                        </select>
                                    </div>
                                    <div class="col-md-6">
                                        <label class="fieldlabels" for="Dusun">Dusun: *</label> 
                                        <input type="text" class="form-control-sm" id="Dusun" name="dusun" placeholder="Dusun" /> 
                                        <label class="fieldlabels" for="rw">RW: *</label> 
                                        <input type="text" class="form-control-sm" id="rw" name="rw" placeholder="RW" /> 
                                        <label class="fieldlabels" for="rt">RT: *</label> 
                                        <input type="text" class="form-control-sm" id="rt" name="rt" placeholder="RT" /> 
                                        <label class="fieldlabels" for="Bencana">Bencana: *</label> 
                                        <input type="text" class="form-control-sm" id="Bencana" name="bencana" placeholder="Bencana" /> 
                                        <label class="fieldlabels" for="Foto">Foto: *</label> 
                                        <input type="file" class="form-control-file" id="Foto" name="foto"/>
                                    </div>
                                </div>
                            </div> 
                            <input type="button" name="next" class="next action-button" value="Next" />
                        </fieldset>
                        <fieldset>
                            <div class="form-card">
                                <div class="row">
                                    <div class="col-7">
                                        <h2 class="fs-title">Informasi Struktur:</h2>
                                    </div>
                                    <div class="col-5">
                                        <h2 class="steps">Step 2 - 5</h2>
                                    </div>
                                    <div class="col-md-12">
                                        <label class="fieldlabels" for="ket_pondasi">Keterangan Pondasi: *</label> 
                                        <input type="text" class="form-control-sm" id="ket_pondasi" name="ket_pondasi" placeholder="Keterangan Pondasi" required/> 
                                        <label class="fieldlabels" for="tkpondasi">Tingkat Kerusakan Pondasi: *</label> 
                                        <input type="text" class="form-control-sm" id="tkpondasi" name="tkpondasi" placeholder="Tingkat Kerusakan Pondasi" required/> 
                                    </div>
                                    <!--Kolom-->
                                    <div class="col-md-6">
                                        <div class="card card-info">
                                            <div class="card-header">
                                                <h3 class="card-title">Struktur Kolom</h3>
                                            </div>
                                            <div class="card-body">
                                                <label class="fieldlabels" for="ket_kolom">Keterangan Kolom: *</label> 
                                                <input type="text" class="form-control-sm" id="ket_kolom" name="ket_kolom" placeholder="Keterangan Kolom" required/> 
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <label class="fieldlabels" for="j_kolom">Jumlah Kolom: *</label> 
                                                        <input type="number" min="0" max="20" class="form-control-sm" id="j_kolom" name="j_kolom" placeholder="Jumlah Kolom" required/> 
                                                        <label class="fieldlabels" for="kolom_tr">Kolom Tidak Rusak: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="kolom_tr" name="kolom_tr" placeholder="Kolom Tidak Rusak" required/> 
                                                        <label class="fieldlabels" for="kolom_rsr">Kolom Rusak Sangat Ringan: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="kolom_rsr" name="kolom_rsr" placeholder="Kolom Rusak Sangat Ringan" required/> 
                                                        <label class="fieldlabels" for="kolom_rr">Kolom Rusak Ringan: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="kolom_rr" name="kolom_rr" placeholder="Kolom Rusak Ringan" required/> 
                                                    </div>
                                                    <div class="col-md-6">
                                                        <label class="fieldlabels" for="kolom_rs">Kolom Rusak Sedang: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="kolom_rs" name="kolom_rs" placeholder="Kolom Rusak Sedang" required/> 
                                                        <label class="fieldlabels" for="kolom_rb">Kolom Rusak Berat: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="kolom_rb" name="kolom_rb" placeholder="Kolom Rusak Berat" required/> 
                                                        <label class="fieldlabels" for="kolom_rsb">Kolom Rusak Sangat Berat: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="kolom_rsb" name="kolom_rsb" placeholder="Kolom Rusak Sangat Berat" required/> 
                                                        <label class="fieldlabels" for="kolom_kts">Komponen Kolom Tidak Sesuai: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="kolom_kts" name="kolom_kts" placeholder="Komponen Kolom Tidak Sesuai" required/> 
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--End Kolom-->
                                    <!--Balok-->
                                    <div class="col-md-6">
                                        <div class="card card-info">
                                            <div class="card-header">
                                                <h3 class="card-title">Struktur Balok</h3>
                                            </div>
                                            <div class="card-body">
                                                <label class="fieldlabels" for="ket_balok">Keterangan Balok: *</label> 
                                                <input type="text" class="form-control-sm" id="ket_balok" name="ket_balok" placeholder="Keterangan Balok" required/> 
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <label class="fieldlabels" for="j_balok">Jumlah Balok: *</label> 
                                                        <input type="number" min="0" max="20" class="form-control-sm" id="j_balok" name="j_balok" placeholder="Jumlah Balok" required/> 
                                                        <label class="fieldlabels" for="balok_tr">Balok Tidak Rusak: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="balok_tr" name="balok_tr" placeholder="Balok Tidak Rusak" required/> 
                                                        <label class="fieldlabels" for="balok_rsr">Balok Rusak Sangat Ringan: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="balok_rsr" name="balok_rsr" placeholder="Balok Rusak Sangat Ringan" required/> 
                                                        <label class="fieldlabels" for="balok_rr">Balok Rusak Ringan: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="balok_rr" name="balok_rr" placeholder="Balok Rusak Ringan" required/> 
                                                    </div>
                                                    <div class="col-md-6">
                                                        <label class="fieldlabels" for="balok_rs">Balok Rusak Sedang: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="balok_rs" name="balok_rs" placeholder="Balok Rusak Sedang" required/> 
                                                        <label class="fieldlabels" for="balok_rb">Balok Rusak Berat: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="balok_rb" name="balok_rb" placeholder="Balok Rusak Berat" required/> 
                                                        <label class="fieldlabels" for="balok_rsb">Balok Rusak Sangat Berat: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="balok_rsb" name="balok_rsb" placeholder="Balok Rusak Sangat Berat" required/> 
                                                        <label class="fieldlabels" for="balok_kts">Komponen Balok Tidak Sesuai: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="balok_kts" name="balok_kts" placeholder="Komponen Balok Tidak Sesuai" required/> 
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--End Balok-->
                                    <!--Atap-->
                                    <div class="col-md-12">
                                        <div class="card card-info">
                                            <div class="card-header">
                                                <h3 class="card-title">Struktur Atap</h3>
                                            </div>
                                            <div class="card-body">
                                                <label class="fieldlabels" for="ket_atap">Keterangan Atap: *</label> 
                                                <input type="text" class="form-control-sm" id="ket_atap" name="ket_atap" placeholder="Keterangan Atap" required/> 
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <label class="fieldlabels" for="v_atap">Volume Atap (%) 100: *</label> 
                                                        <input type="number" min="0" max="20" class="form-control-sm" id="v_atap" name="v_atap" placeholder="Volume Atap (%) 100" readonly/> 
                                                        <label class="fieldlabels" for="atap_tr">Atap Tidak Rusak: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="atap_tr" name="atap_tr" placeholder="Atap Tidak Rusak" required/> 
                                                        <label class="fieldlabels" for="atap_rsr">Atap Rusak Sangat Ringan: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="atap_rsr" name="atap_rsr" placeholder="Atap Rusak Sangat Ringan" required/> 
                                                        <label class="fieldlabels" for="atap_rr">Atap Rusak Ringan: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="atap_rr" name="atap_rr" placeholder="Atap Rusak Ringan" required/> 
                                                    </div>
                                                    <div class="col-md-6">
                                                        <label class="fieldlabels" for="atap_rs">Atap Rusak Sedang: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="atap_rs" name="atap_rs" placeholder="Atap Rusak Sedang" required/> 
                                                        <label class="fieldlabels" for="atap_rb">Atap Rusak Berat: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="atap_rb" name="atap_rb" placeholder="Atap Rusak Berat" required/> 
                                                        <label class="fieldlabels" for="atap_rsb">Atap Rusak Sangat Berat: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="atap_rsb" name="atap_rsb" placeholder="Atap Rusak Sangat Berat" required/> 
                                                        <label class="fieldlabels" for="atap_kts">Komponen Atap Tidak Sesuai: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="atap_kts" name="atap_kts" placeholder="Komponen Atap Tidak Sesuai" required/> 
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--End Atap-->
                                </div> 
                            </div> 
                            <input type="button" name="next" class="next action-button" value="Next" /> 
                            <input type="button" name="previous" class="previous action-button-previous" value="Previous" />
                        </fieldset>
                        <fieldset>
                            <div class="form-card">
                                <div class="row">
                                    <div class="col-7">
                                        <h2 class="fs-title">Informasi Arsitektur:</h2>
                                    </div>
                                    <div class="col-5">
                                        <h2 class="steps">Step 3 - 5</h2>
                                    </div>
                                    <div class="col-md-12">
                                        <!--Dinding/Partisi-->
                                        <label class="fieldlabels" for="ket_dinding">Keterangan Dinding/Partisi: *</label> 
                                        <input type="text" class="form-control-sm" id="ket_dinding" name="ket_dinding" placeholder="Keterangan Dinding/Partisi" required/>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <label class="fieldlabels" for="v_dinding">Volume Dinding (%) 100: *</label> 
                                                <input type="number" min="0" max="20" class="form-control-sm" id="v_dinding" name="v_dinding" placeholder="Volume Dinding (%) 100" readonly/> 
                                                <label class="fieldlabels" for="dinding_tr">Dinding Tidak Rusak: *</label> 
                                                <input type="number" min="0" max="10" class="form-control-sm" id="dinding_tr" name="dinding_tr" placeholder="Dinding Tidak Rusak" required/> 
                                                <label class="fieldlabels" for="dinding_rsr">Dinding Rusak Sangat Ringan: *</label> 
                                                <input type="number" min="0" max="10" class="form-control-sm" id="dinding_rsr" name="dinding_rsr" placeholder="Dinding Rusak Sangat Ringan" required/> 
                                                <label class="fieldlabels" for="dinding_rr">Dinding Rusak Ringan: *</label> 
                                                <input type="number" min="0" max="10" class="form-control-sm" id="dinding_rr" name="dinding_rr" placeholder="Dinding Rusak Ringan" required/> 
                                            </div>
                                            <div class="col-md-6">
                                                <label class="fieldlabels" for="dinding_rs">Dinding Rusak Sedang: *</label> 
                                                <input type="number" min="0" max="10" class="form-control-sm" id="dinding_rs" name="dinding_rs" placeholder="Dinding Rusak Sedang" required/> 
                                                <label class="fieldlabels" for="dinding_rb">Dinding Rusak Berat: *</label> 
                                                <input type="number" min="0" max="10" class="form-control-sm" id="dinding_rb" name="dinding_rb" placeholder="Dinding Rusak Berat" required/> 
                                                <label class="fieldlabels" for="dinding_rsb">Dinding Rusak Sangat Berat: *</label> 
                                                <input type="number" min="0" max="10" class="form-control-sm" id="dinding_rsb" name="dinding_rsb" placeholder="Dinding Rusak Sangat Berat" required/> 
                                                <label class="fieldlabels" for="dinding_kts">Komponen Dinding Tidak Sesuai: *</label> 
                                                <input type="number" min="0" max="10" class="form-control-sm" id="dinding_kts" name="dinding_kts" placeholder="Komponen Dinding Tidak Sesuai" required/> 
                                            </div>
                                        </div>
                                        <!--End Dinding/Partisi-->
                                    </div>
                                    <!--Plafon-->
                                    <div class="col-md-6">
                                        <div class="card card-info">
                                            <div class="card-header">
                                                <h3 class="card-title">Struktur Plafon</h3>
                                            </div>
                                            <div class="card-body">
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <label class="fieldlabels" for="v_plafon">Volume Plafon (%) 100: *</label> 
                                                        <input type="number" min="0" max="20" class="form-control-sm" id="v_plafon" name="v_plafon" placeholder="Volume Plafon (%) 100" readonly/> 
                                                        <label class="fieldlabels" for="plafon_tr">Plafon Tidak Rusak: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="plafon_tr" name="plafon_tr" placeholder="Plafon Tidak Rusak" required/> 
                                                        <label class="fieldlabels" for="plafon_rsr">Plafon Rusak Sangat Ringan: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="plafon_rsr" name="plafon_rsr" placeholder="Plafon Rusak Sangat Ringan" required/> 
                                                        <label class="fieldlabels" for="plafon_rr">Plafon Rusak Ringan: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="plafon_rr" name="plafon_rr" placeholder="Plafon Rusak Ringan" required/> 
                                                    </div>
                                                    <div class="col-md-6">
                                                        <label class="fieldlabels" for="plafon_rs">Plafon Rusak Sedang: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="plafon_rs" name="plafon_rs" placeholder="Plafon Rusak Sedang" required/> 
                                                        <label class="fieldlabels" for="plafon_rb">Plafon Rusak Berat: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="plafon_rb" name="plafon_rb" placeholder="Plafon Rusak Berat" required/> 
                                                        <label class="fieldlabels" for="plafon_rsb">Plafon Rusak Sangat Berat: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="plafon_rsb" name="plafon_rsb" placeholder="Plafon Rusak Sangat Berat" required/> 
                                                        <label class="fieldlabels" for="plafon_kts">Komponen Plafon Tidak Sesuai: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="plafon_kts" name="plafon_kts" placeholder="Komponen Plafon Tidak Sesuai" required/> 
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--End Plafon-->
                                    <!--Lantai-->
                                    <div class="col-md-6">
                                        <div class="card card-info">
                                            <div class="card-header">
                                                <h3 class="card-title">Struktur Lantai</h3>
                                            </div>
                                            <div class="card-body">
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <label class="fieldlabels" for="v_lantai">Volume Lantai (%) 100: *</label> 
                                                        <input type="number" min="0" max="20" class="form-control-sm" id="v_lantai" name="v_lantai" placeholder="Volume Lantai (%) 100" readonly/> 
                                                        <label class="fieldlabels" for="lantai_tr">Lantai Tidak Rusak: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="lantai_tr" name="lantai_tr" placeholder="Lantai Tidak Rusak" required/> 
                                                        <label class="fieldlabels" for="lantai_rsr">Lantai Rusak Sangat Ringan: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="lantai_rsr" name="lantai_rsr" placeholder="Lantai Rusak Sangat Ringan" required/> 
                                                        <label class="fieldlabels" for="lantai_rr">Lantai Rusak Ringan: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="lantai_rr" name="lantai_rr" placeholder="Lantai Rusak Ringan" required/> 
                                                    </div>
                                                    <div class="col-md-6">
                                                        <label class="fieldlabels" for="lantai_rs">Lantai Rusak Sedang: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="lantai_rs" name="lantai_rs" placeholder="Lantai Rusak Sedang" required/> 
                                                        <label class="fieldlabels" for="lantai_rb">Lantai Rusak Berat: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="lantai_rb" name="lantai_rb" placeholder="Lantai Rusak Berat" required/> 
                                                        <label class="fieldlabels" for="lantai_rsb">Lantai Rusak Sangat Berat: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="lantai_rsb" name="lantai_rsb" placeholder="Lantai Rusak Sangat Berat" required/> 
                                                        <label class="fieldlabels" for="lantai_kts">Komponen Lantai Tidak Sesuai: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="lantai_kts" name="lantai_kts" placeholder="Komponen Lantai Tidak Sesuai" required/> 
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--End Lantai-->
                                    <!--Kusen-->
                                    <div class="col-md-6">
                                        <div class="card card-info">
                                            <div class="card-header">
                                                <h3 class="card-title">Struktur Kusen</h3>
                                            </div>
                                            <div class="card-body">
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <label class="fieldlabels" for="j_kusen">Jumlah Kusen: *</label> 
                                                        <input type="number" min="0" max="20" class="form-control-sm" id="j_kusen" name="j_kusen" placeholder="Jumlah Kusen" required/> 
                                                        <label class="fieldlabels" for="kusen_tr">Kusen Tidak Rusak: *</label> 
