                                                        <label class="fieldlabels" for="kusen_rsr">Kusen Rusak Sangat Ringan: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="kusen_rsr" name="kusen_rsr" placeholder="Kusen Rusak Sangat Ringan" required/> 
                                                        <label class="fieldlabels" for="kusen_rr">Kusen Rusak Ringan: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="kusen_rr" name="kusen_rr" placeholder="Kusen Rusak Ringan" required/> 
                                                    </div>
                                                    <div class="col-md-6">
                                                        <label class="fieldlabels" for="kusen_rs">Kusen Rusak Sedang: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="kusen_rs" name="kusen_rs" placeholder="Kusen Rusak Sedang" required/> 
                                                        <label class="fieldlabels" for="kusen_rb">Kusen Rusak Berat: *</label> 
                                                        <input type="number" min="0" max="10" class="form-control-sm" id="kusen_rb" name="kusen_rb" placeholder="Kusen Rusak Berat" required/> 
